{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/quote", "status": 308, "headers": { "Location": "/contact" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}